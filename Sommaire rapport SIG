.active-view-transition.close-sidebar[active-view-transition-type="from-pinned"]::view-transition-old(sidebar) { animation: float-sidebar-out var(--vt-duration,.3s) var(--vt-timing-function,var(--spring-common)) both; height: 100%; object-fit: none; overflow: clip; }

.active-view-transition.close-sidebar[active-view-transition-type="from-pinned"]::view-transition-group(profile-avatar-thread) { display: none; }

.active-view-transition.close-sidebar[active-view-transition-type="from-pinned"]::view-transition-group(profile-avatar-sidebar) { animation: auto ease 0s 1 normal none running none; z-index: 2; }

.active-view-transition.close-sidebar[active-view-transition-type="from-pinned"]::view-transition-new(profile-avatar-sidebar) { animation: auto ease 0s 1 normal none running none; }

.active-view-transition.close-sidebar[active-view-transition-type="from-pinned"]::view-transition-new(thread), .active-view-transition.close-sidebar[active-view-transition-type="from-pinned"]::view-transition-old(thread) { animation-duration: 0.1s; }

.active-view-transition.close-sidebar[active-view-transition-type="from-pinned"]::view-transition-new(thread), .active-view-transition.close-sidebar[active-view-transition-type="from-pinned"]::view-transition-old(thread) { height: 100%; object-fit: none; overflow: clip; }

.active-view-transition.open-sidebar[active-view-transition-type="from-pinned"] { --vt-profile-avatar-sidebar: profile-avatar-active; --vt-profile-avatar-thread: profile-avatar-hidden; --vt_model_picker: model-picker; --vt_toggle_sidebar_closed: toggle-sidebar-icon; --vt_new_chat_thread: new-chat-icon; --vt-composer-speech-button: composer-speech-button; }

.active-view-transition.open-sidebar[active-view-transition-type="from-pinned"].update-callback-done { --vt_toggle_sidebar_closed: none; --vt_toggle_sidebar_opened: toggle-sidebar-icon; --vt_new_chat_sidebar: new-chat-icon; }

@media (max-width: 700px) {
  .active-view-transition.open-sidebar[active-view-transition-type="from-pinned"] { --thread-transition-name: none; --vt_new_chat_thread: none; --vt_new_chat_sidebar: none; }
}

.active-view-transition.open-sidebar[active-view-transition-type="from-pinned"]::view-transition-group(toggle-sidebar-icon) { z-index: 4; }

.active-view-transition.open-sidebar[active-view-transition-type="from-pinned"]::view-transition-old(toggle-sidebar-icon) { height: 100%; object-fit: none; overflow: clip; }

.active-view-transition.open-sidebar[active-view-transition-type="from-pinned"]::view-transition-new(toggle-sidebar-icon) { height: 100%; object-fit: none; overflow: clip; }

.active-view-transition.open-sidebar[active-view-transition-type="from-pinned"]::view-transition-old(new-chat-icon) { display: none; }

.active-view-transition.open-sidebar[active-view-transition-type="from-pinned"]::view-transition-new(new-chat-icon) { animation: auto ease 0s 1 normal none running none; }

.active-view-transition.open-sidebar[active-view-transition-type="from-pinned"]::view-transition-group(new-chat-icon) { z-index: 2; }

.active-view-transition.open-sidebar[active-view-transition-type="from-pinned"]::view-transition-old(sidebar) { display: none; }

.active-view-transition.open-sidebar[active-view-transition-type="from-pinned"]::view-transition-group(sidebar) { animation: auto ease 0s 1 normal none running none; }

.active-view-transition.open-sidebar[active-view-transition-type="from-pinned"]::view-transition-new(sidebar) { animation: float-sidebar-in var(--vt-duration,.3s) var(--vt-timing-function,var(--spring-common)) both; height: 100%; object-fit: none; overflow: clip; }

.active-view-transition.open-sidebar[active-view-transition-type="from-pinned"]::view-transition-group(thread) { --vt-timing-function: var(--spring-common); }

.active-view-transition.open-sidebar[active-view-transition-type="from-pinned"]::view-transition-new(thread), .active-view-transition.open-sidebar[active-view-transition-type="from-pinned"]::view-transition-old(thread) { height: 100%; object-fit: none; overflow: clip; }
