    <span class="hljs-keyword">def</span> <span class="hljs-title function_">__str__</span>(<span class="hljs-params">self</span>):
        <span class="hljs-keyword">return</span> self.status<font></font>
<font></font>
<span class="hljs-keyword">class</span> <span class="hljs-title class_">IndicatorValue</span>(models.Model):<font></font>
    indicator = models.ForeignKey(Indicator, on_delete=models.CASCADE)<font></font>
    country = models.ForeignKey(Country, null=<span class="hljs-literal">True</span>, blank=<span class="hljs-literal">True</span>, on_delete=models.CASCADE)<font></font>
    region = models.ForeignKey(Region, null=<span class="hljs-literal">True</span>, blank=<span class="hljs-literal">True</span>, on_delete=models.CASCADE)<font></font>
    department = models.ForeignKey(Department, null=<span class="hljs-literal">True</span>, blank=<span class="hljs-literal">True</span>, on_delete=models.CASCADE)<font></font>
    disaggregation_value = models.ForeignKey(DisaggregationValue, null=<span class="hljs-literal">True</span>, blank=<span class="hljs-literal">True</span>, on_delete=models.CASCADE)<font></font>
    collection_period = models.ForeignKey(CollectionPeriod, on_delete=models.CASCADE)<font></font>
    value = models.DecimalField(max_digits=<span class="hljs-number">10</span>, decimal_places=<span class="hljs-number">2</span>)<font></font>
    status = models.ForeignKey(IndicatorValueStatus, on_delete=models.CASCADE)<font></font>
    created_at = models.DateTimeField(auto_now_add=<span class="hljs-literal">True</span>)<font></font>
<font></font>
    <span class="hljs-keyword">def</span> <span class="hljs-title function_">__str__</span>(<span class="hljs-params">self</span>):
        <span class="hljs-keyword">return</span> <span class="hljs-string">f"<span class="hljs-subst">{self.indicator.name}</span> - <span class="hljs-subst">{self.value}</span>"</span><font></font>
<font></font>
    <span class="hljs-keyword">def</span> <span class="hljs-title function_">save</span>(<span class="hljs-params">self, *args, **kwargs</span>):
        <span class="hljs-keyword">if</span> self.disaggregation_value:<font></font>
            related_values = IndicatorValue.objects.<span class="hljs-built_in">filter</span>(<font></font>
                indicator=self.indicator,<font></font>
                disaggregation_value=self.disaggregation_value,<font></font>
                status__status=<span class="hljs-string">'Validé'</span><font></font>
            )<font></font>
            self.value = <span class="hljs-built_in">sum</span>(rv.value <span class="hljs-keyword">for</span> rv <span class="hljs-keyword">in</span> related_values)
        <span class="hljs-built_in">super</span>().save(*args, **kwargs)
</code></div></div></pre><h3><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Mise à jour des Serializers</font></font></h3><p><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Les sérialiseurs restent les mêmes, sauf pour l'ajout de champs spécifiques aux nouvelles exigences.</font></font></p><pre><div class="dark bg-gray-950 rounded-md border-[0.5px] border-token-border-medium"><div class="flex items-center relative text-token-text-secondary bg-token-main-surface-secondary px-4 py-2 text-xs font-sans justify-between rounded-t-md"><span><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">python</font></font></span><div class="flex items-center"><span class="" data-state="closed"><button class="flex gap-1 items-center"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none" viewBox="0 0 24 24" class="icon-sm"><path fill="currentColor" fill-rule="evenodd" d="M7 5a3 3 0 0 1 3-3h9a3 3 0 0 1 3 3v9a3 3 0 0 1-3 3h-2v2a3 3 0 0 1-3 3H5a3 3 0 0 1-3-3v-9a3 3 0 0 1 3-3h2zm2 2h5a3 3 0 0 1 3 3v5h2a1 1 0 0 0 1-1V5a1 1 0 0 0-1-1h-9a1 1 0 0 0-1 1zM5 9a1 1 0 0 0-1 1v9a1 1 0 0 0 1 1h9a1 1 0 0 0 1-1v-9a1 1 0 0 0-1-1z" clip-rule="evenodd"></path></svg><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Copier le code</font></font></button></span></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="!whitespace-pre hljs language-python"><span class="hljs-keyword">from</span> rest_framework <span class="hljs-keyword">import</span> serializers
