    serializer_class = IndicatorValueSerializer<font></font>
<font></font>
    <span class="hljs-keyword">def</span> <span class="hljs-title function_">get_queryset</span>(<span class="hljs-params">self</span>):<font></font>
        indicator_id = self.request.query_params.get(<span class="hljs-string">'indicator_id'</span>)
        <span class="hljs-keyword">return</span> IndicatorValue.objects.<span class="hljs-built_in">filter</span>(indicator_id=indicator_id, status__status=<span class="hljs-string">'Validé'</span>)
