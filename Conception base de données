    country = models.ForeignKey(Country, on_delete=models.CASCADE)<font></font>
<font></font>
    <span class="hljs-keyword">def</span> <span class="hljs-title function_">__str__</span>(<span class="hljs-params">self</span>):
        <span class="hljs-keyword">return</span> self.name<font></font>
<font></font>
<span class="hljs-keyword">class</span> <span class="hljs-title class_">Department</span>(models.Model):<font></font>
