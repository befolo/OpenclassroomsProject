<font></font>
<span class="hljs-keyword">class</span> <span class="hljs-title class_">Indicator</span>(models.Model):<font></font>
    SCALE_CHOICES = [<font></font>
        (<span class="hljs-string">'country'</span>, <span class="hljs-string">'Country'</span>),<font></font>
        (<span class="hljs-string">'region'</span>, <span class="hljs-string">'Region'</span>),<font></font>
        (<span class="hljs-string">'department'</span>, <span class="hljs-string">'Department'</span>),<font></font>
    ]<font></font>
    name = models.CharField(max_length=<span class="hljs-number">100</span>)<font></font>
    description = models.TextField()<font></font>
    collection_scale = models.CharField(max_length=<span class="hljs-number">10</span>, choices=SCALE_CHOICES)<font></font>
<font></font>
    <span class="hljs-keyword">def</span> <span class="hljs-title function_">__str__</span>(<span class="hljs-params">self</span>):
        <span class="hljs-keyword">return</span> self.name<font></font>
<font></font>
    <span class="hljs-keyword">def</span> <span class="hljs-title function_">get_default_value</span>(<span class="hljs-params">self</span>):<font></font>
        disaggregations = IndicatorValue.objects.<span class="hljs-built_in">filter</span>(indicator=self, status__status=<span class="hljs-string">'Validé'</span>)<font></font>
        total = <span class="hljs-built_in">sum</span>(d.value <span class="hljs-keyword">for</span> d <span class="hljs-keyword">in</span> disaggregations <span class="hljs-keyword">if</span> d.disaggregation_value <span class="hljs-keyword">is</span> <span class="hljs-keyword">not</span> <span class="hljs-literal">None</span>)
        <span class="hljs-keyword">return</span> total <span class="hljs-keyword">if</span> total <span class="hljs-keyword">else</span> <span class="hljs-literal">None</span><font></font>
<font></font>
    <span class="hljs-keyword">def</span> <span class="hljs-title function_">clean</span>(<span class="hljs-params">self</span>):
        <span class="hljs-keyword">if</span> IndicatorValue.objects.<span class="hljs-built_in">filter</span>(indicator=self).exists():
            <span class="hljs-keyword">raise</span> ValidationError(<span class="hljs-string">"An indicator cannot have multiple collection scales."</span>)<font></font>
<font></font>
<span class="hljs-keyword">class</span> <span class="hljs-title class_">DisaggregationType</span>(models.Model):<font></font>
    name = models.CharField(max_length=<span class="hljs-number">100</span>)<font></font>
    collection_scale = models.CharField(max_length=<span class="hljs-number">10</span>, choices=Indicator.SCALE_CHOICES)<font></font>
<font></font>
    <span class="hljs-keyword">def</span> <span class="hljs-title function_">__str__</span>(<span class="hljs-params">self</span>):
        <span class="hljs-keyword">return</span> self.name<font></font>
<font></font>
<span class="hljs-keyword">class</span> <span class="hljs-title class_">DisaggregationValue</span>(models.Model):
    <span class="hljs-built_in">type</span> = models.ForeignKey(DisaggregationType, on_delete=models.CASCADE)<font></font>
